{
  "name": "world-of-reos",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run dev-angular\" \"npm run dev-express\"",
    "dev-angular": "cd angular && ng serve --proxy-config proxy.conf.json && cd ..",
    "dev-express": "cd express && ts-node-dev --respawn --transpileOnly ./src/server.ts && cd ..",
    "build-angular": "pushd angular && ng build --prod && popd",
    "build-express": "pushd express && tsc && popd",
    "build": "npm run build-angular && npm run build-express",
    "prod": "node ./dist/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
