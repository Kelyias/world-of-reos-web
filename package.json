    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@types/chart.js": "^2.8.4",
    "animate.css": "^3.7.2",
